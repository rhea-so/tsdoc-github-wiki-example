{
  "$schema": "https://typedoc.org/schema.json",
  "out": "./wiki",
  "name": "TSDoc GitHub Wiki Example",
  "entryPoints": ["src/**/*.ts"],
  "exclude": ["src/**/index.ts", "src/**/*.spec.ts"],
  "plugin": [
    "typedoc-plugin-missing-exports",
    "typedoc-plugin-markdown",
    "typedoc-github-wiki-theme"
  ],
  "githubPages": false
}
